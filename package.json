{
  "name": "@miaooo/nestjs-take-type",
  "version": "1.2.0",
  "description": "NestJS helper function that combined PartialType and PickType",
  "main": "lib/src/index.js",
  "module": "es/src/index.js",
  "types": "lib/src/index.d.ts",
  "keywords": [
    "nestjs",
    "swagger",
    "type",
    "partial",
    "pick",
    "take",
    "TakeType",
    "PartialType",
    "PickType"
  ],
  "scripts": {
    "test": "npm run clean && npm run build:test && nyc ava",
    "lint": "eslint \"src/**/*.ts\" \"tests/**/*.ts\"  --fix",
    "prepare": "is-ci || husky install",
    "ca": "git add -A && git-cz -av",
    "ci": "git-cz",
    "release": "standard-version",
    "release:alpha": "standard-version --prerelease alpha",
    "release:first": "standard-version --first-release",
    "test:debug": "ava --verbose",
    "test:update": "ava -u",
    "report": "npm run build:test && nyc --reporter=html --reporter=text-summary --skip-full ava",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf ./lib/* && rm -rf ./es/*",
    "prebuild": "npm run clean",
    "build:es": "ttsc -p build/tsconfig.es.json && npm run copyfile:es",
    "build:lib": "ttsc -p build/tsconfig.lib.json && npm run copyfile:lib",
    "build:test": "ttsc -p build/tsconfig.test.json && npm run copyfile:lib",
    "copyfile:es": "copyfiles -u 1 \"src/**/*.!(ts)\" es/src",
    "copyfile:lib": "copyfiles -u 1 \"src/**/*.!(ts)\" lib/src",
    "build": "npm run build:lib && npm run build:es",
    "prewatch": "npm run copyfile:lib",
    "watch": "NODE_ENV=development ttsc -p build/tsconfig.lib.json -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Val-istar-Guo/nestjs-take-type.git"
  },
  "author": "Val.istar.Guo <val.istar.guo@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Val-istar-Guo/nestjs-take-type/issues"
  },
  "homepage": "https://github.com/Val-istar-Guo/nestjs-take-type#readme",
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@nestjs/common": "^9.1.4",
    "@nestjs/core": "^9.1.4",
    "@nestjs/swagger": "^6.1.2",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "ajv": "8",
    "ava": "^4.3.0",
    "commitizen": "^4.2.4",
    "copyfiles": "^2.4.1",
    "coveralls": "^3.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.20.0",
    "husky": "^5.2.0",
    "is-ci": "^3.0.0",
    "lint-staged": "^11.0.0",
    "mili": "^4.7.5",
    "nyc": "^15.1.0",
    "reflect-metadata": "*",
    "rxjs": "^7.5.7",
    "sinon": "^14.0.0",
    "standard-version": "^9.3.0",
    "ts-node": "^10.8.1",
    "ttypescript": "^1.5.13",
    "typescript": "^4.7.4",
    "typescript-transform-paths": "^3.3.1"
  },
  "dependencies": {
    "@types/node": ">=14"
  },
  "peerDependencies": {
    "@nestjs/common": "^8 || ^9 || ^10",
    "@nestjs/core": "^8 || ^9 || ^10",
    "@nestjs/swagger": "^5 || ^6 || ^7",
    "reflect-metadata": "*",
    "rxjs": ">=7"
  }
}
